# Production Environment Configuration
# Deze waarden moeten worden ingevuld met productie Supabase project gegevens

VITE_SUPABASE_PROJECT_ID="YOUR_PRODUCTION_PROJECT_ID"
VITE_SUPABASE_URL="https://YOUR_PRODUCTION_PROJECT_ID.supabase.co"
VITE_SUPABASE_ANON_KEY="YOUR_PRODUCTION_ANON_KEY"

# Security Configuration (stricter voor productie)
VITE_SESSION_TIMEOUT_MINUTES=20
VITE_RATE_LIMIT_ENABLED=true

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PWA=true

# Environment
NODE_ENV=production
VITE_APP_ENV=production