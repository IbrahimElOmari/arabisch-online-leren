# Staging Environment Configuration
# Deze waarden moeten worden ingevuld met staging Supabase project gegevens

VITE_SUPABASE_PROJECT_ID="YOUR_STAGING_PROJECT_ID"
VITE_SUPABASE_URL="https://YOUR_STAGING_PROJECT_ID.supabase.co"
VITE_SUPABASE_ANON_KEY="YOUR_STAGING_ANON_KEY"

# Security Configuration
VITE_SESSION_TIMEOUT_MINUTES=25
VITE_RATE_LIMIT_ENABLED=true

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_OFFLINE_MODE=false
VITE_ENABLE_PWA=true

# Environment
NODE_ENV=production
VITE_APP_ENV=staging